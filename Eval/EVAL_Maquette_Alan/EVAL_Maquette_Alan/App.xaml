<Application
    x:Class="EVAL_Maquette_Alan.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:EVAL_Maquette_Alan"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <LinearGradientBrush x:Key="bouton" StartPoint="0.5,0" EndPoint="0.5,1">

        <GradientStop Offset="0" Color="gray" />
        <GradientStop Offset="1" Color="white" />
        <GradientStop Offset="2" Color="gray" />
        <GradientStop Offset="3" Color="white" />

    </LinearGradientBrush>
    
    </Application.Resources >
</Application>
